{
  "name": "tested-todos",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "yarn testdb:up && yarn testdb:migrate && dotenv -e .env.test jest -- --watch",
    "test:unit": "yarn testdb:up && yarn testdb:migrate && dotenv -e .env.test jest -i",
    "testdb:migrate": "dotenv -e .env.test -- npx prisma migrate dev --name postgres-init",
    "testdb:up": "docker-compose up -d",
    "testdb:down": "docker-compose down",
    "studio": "dotenv -e .env.local -- npx prisma studio"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^1.8.0",
    "@hookform/resolvers": "^2.8.8",
    "@prisma/client": "^3.14.0",
    "next": "12.1.6",
    "next-themes": "^0.2.0",
    "prisma": "^3.14.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-hook-form": "^7.31.1",
    "swr": "^1.3.0",
    "zod": "^3.16.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@types/node": "17.0.32",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.3",
    "autoprefixer": "^10.4.7",
    "dotenv-cli": "^5.1.0",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "node-mocks-http": "^1.11.0",
    "postcss": "^8.4.13",
    "tailwindcss": "^3.0.24",
    "ts-node": "^10.7.0",
    "typescript": "4.6.4"
  }
}
